<!DOCTYPE html>
<html lang="ja">
  <head>
    <meta charset="utf-8">
    <title>ログイン</title>
    <link rel="stylesheet" href="${request.static_url('translateapp:html/css/login.css') }"/>
    <link rel="stylesheet" href="${request.static_url('translateapp:html/css/bootstrap.min.css') }"/>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <script src="/html/js/login.js"></script>
    <script src="/html/js/bootstrap.min.js"></script>
  </head>
  <body>
    <div class="container" id="login-form">
      <div class="row">
        <div class="form col-md-5 col-md-offset-3">
          <p class="form-title text-center">ログイン</p>
          <div class="form-group">
            <p>名前<input type="name" id='name' class="form-control"></p>
          </div>
          <div class="form-group">
            <p>パスワード<input type="password" id='password' class="form-control"></p>
          </div>
          <div class="form-group">
            <input type="submit" id='submit' class="btn btn-primary center-block" value="ログイン">
          </div>
        </div>
      </div>
    </div>
  </body>
</html>

